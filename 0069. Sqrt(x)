https://leetcode.com/problems/sqrtx/

class Solution {
public:
    int mySqrt(int x) {
        if(x==0) return 0;
        int l = 1,r =x,ans=0;
        while(l<=r){
            int mid = l +(r-l)/2;
            
            if(x/mid==mid) return mid;
            else if(mid > x/mid) r = mid-1;
            else{
                l = mid+1;
                ans = mid;
            }
        }
        return ans;
    }
};
